extends layout
block content
 // GenesisUI main scripts

 main.main
        // Breadcrumb
        ol.breadcrumb
          li.breadcrumb-item Home
          li.breadcrumb-item
            a(href='#') Admin
          li.breadcrumb-item.active Dashboard
        .container-fluid
          .animated.fadeIn
            .row
              .col-sm-6.col-lg-3
                .card.text-white.bg-primary
                  .card-body.pb-0
                    .btn-group.float-right
                      button.btn.btn-transparent.dropdown-toggle.p-0(type='button', data-toggle='dropdown', aria-haspopup='true', aria-expanded='false')
                        i.icon-settings
                      .dropdown-menu.dropdown-menu-right
                        a.dropdown-item(href='#') Detalhes
                    //- Develop EndPoint - Count TripMileage 
                    h4.mb-0 27,34 Km
                        #SumCurMileage                              
                    p Distância Percorrida
                  .chart-wrapper.px-3(style='height:70px;')
                    canvas#card-chart1.chart(height='70')
              // /.col
              .col-sm-6.col-lg-3
                .card.text-white.bg-info
                  .card-body.pb-0
                    .btn-group.float-right
                      button.btn.btn-transparent.dropdown-toggle.p-0(type='button', data-toggle='dropdown', aria-haspopup='true', aria-expanded='false')
                        i.icon-settings
                      .dropdown-menu.dropdown-menu-right
                        a.dropdown-item(href='#') Detalhes
                    //- Develop EndPoint - Count Alarms Idle Engine (Sum alarm time) 
                    h4.mb-0 60 minutos
                    p Motor Ocioso
                  .chart-wrapper.px-3(style='height:70px;')
                    canvas#card-chart2.chart(height='70')
              // /.col
              .col-sm-6.col-lg-3
                .card.text-white.bg-warning
                  .card-body.pb-0
                    .btn-group.float-right
                      button.btn.btn-transparent.dropdown-toggle.p-0(type='button', data-toggle='dropdown', aria-haspopup='true', aria-expanded='false')
                        i.icon-settings
                      .dropdown-menu.dropdown-menu-right
                        a.dropdown-item(href='#') Detalhes
                    //- Develop EndPoint - Count Alarms Accelaration (Sum alarm time)    
                    h4.mb-0 0 minutos
                    p Altas Acelerações
                  .chart-wrapper(style='height:70px;')
                    canvas#card-chart3.chart(height='70')
              // /.col
              .col-sm-6.col-lg-3
                .card.text-white.bg-danger
                  .card-body.pb-0
                    .btn-group.float-right
                      button.btn.btn-transparent.dropdown-toggle.p-0(type='button', data-toggle='dropdown', aria-haspopup='true', aria-expanded='false')
                        i.icon-settings
                      .dropdown-menu.dropdown-menu-right
                        a.dropdown-item(href='#') Detalhes
                    //- Develop EndPoint - Count Alarms Harsh Brake (Sum alarm time)        
                    h4.mb-0 0 minutos
                    p Freadas bruscas
                  .chart-wrapper.px-3(style='height:70px;')
                    canvas#card-chart4.chart(height='70')
            .row
              .col-md-12
                .card
                  .card-header
                    | Veiculos - Mensal
                  .card-body
                    .row
                      .col-sm-12.col-lg-4
                        .row
                          .col-sm-6
                            .callout.callout-info
                              small.text-muted Conectados
                              br
                              //- EndPoint - Provide Total Connected (login recente or alarm conected)
                              strong.h4 1
                              .chart-wrapper
                                canvas#sparkline-chart-1(width='100', height='30')
                          // /.col
                          .col-sm-6
                            .callout.callout-danger
                              small.text-muted Desconectados
                              br
                              //- EndPoint - Provide Total Disconnected (login recente or alarm disconected)
                              strong.h4 1
                              .chart-wrapper
                                canvas#sparkline-chart-2(width='100', height='30')
                      .col-sm-6.col-lg-4
                        .row
                          .col-sm-6
                            .callout.callout-warning
                              small.text-muted SOS
                              br
                              //- EndPoint - Provide Total SOS Alarms
                              strong.h4 0
                              .chart-wrapper
                                canvas#sparkline-chart-3(width='100', height='30')
                          // /.col
                          .col-sm-6
                            .callout.callout-success
                              small.text-muted Rebocados
                              br
                              //- EndPoint - Provide Total Towing alarms
                              strong.h4 0
                              .chart-wrapper
                                canvas#sparkline-chart-4(width='100', height='30')
                          // /.col                        
                      .col-sm-6.col-lg-4
                        .row
                          .col-sm-6
                            .callout
                              small.text-muted Combustível
                              br
                              //- EndPoint - Provide Total Speeding alarms
                              strong.h4 10 L
                              .chart-wrapper
                                canvas#sparkline-chart-5(width='100', height='30')
                          // /.col
                          .col-sm-6
                            .callout.callout-primary
                              small.text-muted MIL*
                              br
                              //- EndPoint - Provide Total MIL alarms
                              strong.h4 0
                              .chart-wrapper
                                canvas#sparkline-chart-6(width='100', height='30')
                          // /.col                        
                    br
                    table.table.table-responsive.table-hover.table-outline.mb-0
                      thead.thead-default
                        tr
                          th.text-center
                            i.icon-people
                          th Veículos
                          th.text-center Cliente
                          th Uso
                          th.text-center Perfil
                          th Atividades
                      tbody
                        tr
                          td.text-center
                            .avatar
                              img.img-avatar(src='img/avatars/1.jpg', alt='admin@bootstrapmaster.com')
                              span.avatar-status.badge-success
                          td
                            div 
                               a(href='/analytics') JXA-0944
                            .small.text-muted
                              span Novo
                              | | Registrado: 2017-09-02
                          td.text-center
                            span Viriato
                          td
                            .clearfix
                              .float-left
                                strong 20%
                              .float-right
                                //- small.text-muted #{params.CurWStart} ~ #{params.CurWEnd}                                                                  
                            .progress.progress-xs
                              .progress-bar.bg-success(role='progressbar', style='width: 50%', aria-valuenow='50', aria-valuemin='0', aria-valuemax='100')
                          td.text-center
                            i.fa.fa-star(style='font-size:24px')
                            i.fa.fa-star(style='font-size:24px')
                            i.fa.fa-star(style='font-size:24px')
                            i.fa.fa-star(style='font-size:24px')
                            i.fa.fa-star(style='font-size:24px')
                          td
                            .small.text-muted Último login
                            strong 7 dias atrás